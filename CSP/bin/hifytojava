#!/usr/bin/perl

# Demonstration of how to invoke HifyToJava: the first
# argument is the name of the Java package into which
# the generated Java code should be placed.  The
# second argument is the filesystem directory corresponding
# to that package.
#
# Jonathan Crabtree 
#

require CBIL::CSP::HifyToJava;
CBIL::CSP::HifyToJava::generateJavaHify("cbil.csp", "../../java/cbil/csp/");

